class Solution {
private:
    void ss(int ind,vector <int>&nums, vector <int> &ds,set <vector <int>> &ans){
        if (ind==nums.size()){
            ans.insert(ds);
            return;
        }
        ds.push_back(nums[ind]);
        ss(ind+1,nums,ds,ans);
        ds.pop_back();
        ss(ind+1,nums,ds,ans);
    }
public:
    vector<vector<int>> subsetsWithDup(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        vector <int> ds;
        set <vector <int>> ans;
        ss(0,nums,ds,ans);
        vector <vector <int>> result;
        for (auto it:ans){
            result.push_back(it);
        }
        return result;
    }
};
