class Solution {
public:
    string largestOddNumber(string num) {
        int j=-1;
        for (int i=num.size()-1;i>=0;i--){
            if ((num[i]-'0')%2==1){
                j=i;
                break;
            }
        }
        if (j==-1){
            return "";
        }
        if (j==num.size()-1){
            return num;
        }
        ostringstream s;
        for (int i=0;i<=j;i++){
            s<<num[i];
        }
        return s.str();

    }
};
