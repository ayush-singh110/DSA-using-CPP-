class Solution {
public:
    int findKthLargest(vector<int>& nums, int k) {
        if (k>nums.size()){
            return 0;
        }
        priority_queue<int> q;
        for (auto it:nums){
            q.push(it);
        }
        int j=0;
        for (int i=0;i<k;i++){
            if (i==k-1){
                j=q.top();
                q.pop();
                break;
            }
            q.pop();
        }
        return j;
    }
};
