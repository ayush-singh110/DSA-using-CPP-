class Solution {
private:
    const static long long m=1000000007;
    long long pown(long long x,long long n){
        if (n==0){
            return 1;
        }
        long long half=pown(x,n/2);
        if (n%2==0){
            return (half*half)%m;
        }
        return ((half*half)%m*x)%m;
    }
public:
    int countGoodNumbers(long long n) {
        long long even=(n+1)/2;
        long long odd=n/2;
        long long result=(pown(5,even)*pown(4,odd))%m;
        return result;
    }
};
